    <script type="text/javascript">
      google.load('visualization', '1');
      google.setOnLoadCallback(drawVisualization);
      function drawVisualization() {
		  
		  // --------------DAILY CHARTS------------------------
        var wrapper = new google.visualization.ChartWrapper({
          chartType: 'LineChart',
          dataSourceUrl: 'https://docs.google.com/spreadsheets/d/1CpVtXz8RdvA5h67pcHqUi4vXPBPi5cOoRFOvIZOEUUs/edit#gid=0',
          query: 'SELECT A,B ORDER BY A DESC LIMIT 96',
          options: {'title': 'Sensor-1', 'backgroundColor': 'transparent', 'colors': ['#4588A5'], 'explorer': { axis: 'horizontal' }},
          containerId: 'sensor1grafikdaily'
        });
        wrapper.draw()
		//---OTHER DAILY CHARTS-----		

		//--------------------WEEKLY CHARTS----------------------
        var wrapper = new google.visualization.ChartWrapper({
          chartType: 'LineChart',
          dataSourceUrl: 'https://docs.google.com/spreadsheets/d/1CpVtXz8RdvA5h67pcHqUi4vXPBPi5cOoRFOvIZOEUUs/edit#gid=0',
          query: 'SELECT A,B ORDER BY A DESC LIMIT 672',
          options: {'title': 'Sensor-1', 'backgroundColor': 'transparent', 'colors': ['#4588A5'], 'explorer': { axis: 'horizontal' }},
          containerId: 'sensor1grafikweekly'
        });
        wrapper.draw()
		//---OTHER WEEKLY CHARTS-----	

